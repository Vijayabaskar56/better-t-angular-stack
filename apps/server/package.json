{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "check-types": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/src/index.js",
    "db:push": "prisma db push --schema ./prisma/schema",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@trpc/server": "^11.0.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.2",
    "hono": "^4.7.5",
    "@hono/trpc-server": "^0.3.4",
    "@hono/node-server": "^1.14.0",
    "@prisma/client": "^6.5.0",
    "better-auth": "^1.2.4",
    "ai": "^4.2.8",
    "@ai-sdk/google": "^1.2.3"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2",
    "tsx": "^4.19.2",
    "@types/node": "^22.13.11",
    "prisma": "^6.5.0"
  }
}
